@model IEnumerable<int>

@Html.ActionLink("Домашня сторінка", "Index", "Home")

@foreach (int link in Model)
{
@Html.RouteLink("Кімната "+link.ToString(), new
{
    controller = "Home",
    action = "List",
    Rooms = link,
    Room = link.ToString()
},new {
@class = link == Convert.ToInt32(ViewBag.room) ? "selected" : null
})
}

@Html.ActionLink("Інше", "OtherActions", "Home")